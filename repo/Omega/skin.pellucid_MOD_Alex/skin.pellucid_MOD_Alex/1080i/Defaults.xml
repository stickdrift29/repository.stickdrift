<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label></label>
        <height>1-row</height>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>0</textoffsetx>
        <font>regular</font>
        <textcolor>textActive</textcolor>
        <focusedcolor>textActive</focusedcolor>
        <selectedcolor>textActive</selectedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <shadowcolor></shadowcolor>
    </default>
    <default type="textbox">
        <label></label>
        <font>regular</font>
        <textcolor>textActive</textcolor>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <height>57</height>
        <label></label>
        <align>left</align>
        <aligny>center</aligny>
        <font>regular</font>
        <textcolor>textActive</textcolor>
        <textoffsetx>0</textoffsetx>
        <focusedcolor>textActive</focusedcolor>
        <selectedcolor>textActive</selectedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>2000</pauseatend>
    </default>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width></width>
        <height></height>
        <align>center</align>
        <aligny>center</aligny>
        <texture></texture>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <align>left</align>
        <aligny>center</aligny>
        <height>2-row</height>
        <width>4-col</width>
        <font>buttonTitle</font>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <texturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</texturefocus>
        <texturenofocus></texturenofocus>
        <textoffsetx>24</textoffsetx>
        <disabledcolor>textDisabled</disabledcolor>
        <invalidcolor>textDisabled</invalidcolor>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>2-col</width>
        <height>2-row</height>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>24</textoffsetx>
        <label></label>
        <texturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</texturefocus>
        <texturenofocus></texturenofocus>
        <alttexturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</alttexturefocus>
        <alttexturenofocus></alttexturenofocus>
        <font>buttonTitle</font>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <colordiffuse>ffFFFFFF</colordiffuse>
    </default>
    <default type="radiobutton">
        <posx>0</posx>
        <posy>0</posy>
        <width></width>
        <height>2-row</height>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>24</textoffsetx>
        <textoffsety>0</textoffsety>
        <font>buttonTitle</font>
        <label></label>
        <label2></label2>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <radioposx>0</radioposx>
        <radioposy>0</radioposy>
        <texturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</texturefocus>
        <texturenofocus></texturenofocus>
        <pulseonselect>false</pulseonselect>
        <colordiffuse>FFFFFFFF</colordiffuse>
        <textureradioonfocus colordiffuse="black">buttons/radio-button-on.png</textureradioonfocus>
        <textureradioonnofocus colordiffuse="textActive">buttons/radio-button-on.png</textureradioonnofocus>
        <textureradioofffocus colordiffuse="black">buttons/radio-button-off.png</textureradioofffocus>
        <textureradiooffnofocus colordiffuse="textActive">buttons/radio-button-off.png</textureradiooffnofocus>
        <radiowidth>120</radiowidth>
		<radioheight>76</radioheight>
    </default>
    <default type="spincontrolex">
        <posx>0</posx>
        <posy>0</posy>
        <width></width>
        <height>2-row</height>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>24</textoffsetx>
        <font>buttonTitle</font>
        <label></label>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <spinwidth>30</spinwidth>
        <spinheight>30</spinheight>
        <texturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</texturefocus>
        <texturenofocus></texturenofocus>
        <textureup>UIControls/arrow-up-nf.png</textureup>
        <texturedown>UIControls/arrow-down-nf.png</texturedown>
        <textureupfocus>UIControls/arrow-up-fo.png</textureupfocus>
        <texturedownfocus>UIControls/arrow-down-fo.png</texturedownfocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <width></width>
        <height>2-row</height>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>24</textoffsetx>
        <font>buttonTitle</font>
        <label></label>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <spinwidth>36</spinwidth>
        <spinheight>36</spinheight>
        <texturefocus>masks/discsurface.png</texturefocus>
        <texturenofocus></texturenofocus>
        <textureup>UIControls/arrow-up-nf.png</textureup>
        <texturedown>UIControls/arrow-down-nf.png</texturedown>
        <textureupfocus>UIControls/arrow-up-fo.png</textureupfocus>
        <texturedownfocus>UIControls/arrow-down-fo.png</texturedownfocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <!--used in audio settings etc-->
    <default type="sliderex">
        <posx>0</posx>
        <posy>0</posy>
        <width></width>
        <height>2-row</height>
        <aligny>center</aligny>
        <font>buttonTitle</font>
        <textoffsetx>24</textoffsetx>
        <textoffsety>0</textoffsety>
        <textcolor>textActive</textcolor>
        <focusedcolor>black</focusedcolor>
        <disabledcolor>textDisabled</disabledcolor>
        <texturenofocus></texturenofocus>
        <texturefocus colordiffuse="$VAR[selected_box]">masks/discsurface.png</texturefocus>
        <sliderwidth>0</sliderwidth>
        <sliderheight>0</sliderheight>
        <texturesliderbar></texturesliderbar>
        <textureslidernib></textureslidernib>
        <textureslidernibfocus></textureslidernibfocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="progress">
        <height>12</height>
        <texturebg colordiffuse="overlay">masks/discsurface.png</texturebg>
        <midtexture colordiffuse="$VAR[highlight]">masks/discsurface.png</midtexture>
        <righttexture></righttexture>
        <overlaytexture></overlaytexture>
        <reveal>true</reveal>
    </default>
    <default type="scrollbar">
        <width>12</width>
        <texturesliderbar></texturesliderbar>
        <texturesliderbar colordiffuse="textDisabled">masks/discsurface.png</texturesliderbar>
        <texturesliderbackground colordiffuse="black">masks/discsurface.png</texturesliderbackground>
        <texturesliderbarfocus colordiffuse="$VAR[highlight]">masks/discsurface.png</texturesliderbarfocus>
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>
    <default type="edit">
        <width>8-col</width>
        <height>2-row</height>
        <font>mediumStrong</font>
        <textcolor>textActive</textcolor>
        <disabledcolor>textActive</disabledcolor>
        <invalidcolor>textActive</invalidcolor>
        <focusedcolor>textActive</focusedcolor>
        <textoffsetx>12</textoffsetx>
        <textoffsety>0</textoffsety>
        <align>center</align>
        <aligny>center</aligny>
        <shadowcolor></shadowcolor>
        <texturefocus colordiffuse="textboxInactive">colors/black.png</texturefocus>
        <texturenofocus colordiffuse="overlay">colors/black.png</texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="colorbutton">
		<textcolor>textActive</textcolor>
		<focusedcolor>black</focusedcolor>
		<disabledcolor>overlay</disabledcolor>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
		<width>1381</width>
		<height>81</height>
		<colorwidth>100</colorwidth>
		<colorheight>40</colorheight>
		<texturecolormask>masks/discsurface.png</texturecolormask>
		<texturecolordisabledmask colordiffuse="40FFFFFF">masks/discsurface.png</texturecolordisabledmask>
		<font>buttonTitle</font>
		<textoffsetx>30</textoffsetx>
		<texturefocus colordiffuse="$VAR[selected_box]" border="3">masks/discsurface.png</texturefocus>
	</default>
</includes>
